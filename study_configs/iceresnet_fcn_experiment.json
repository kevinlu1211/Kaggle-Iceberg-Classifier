{
  "model": {
    "name": "IceResNetFCN",
    "model_configuration_name": "IceResNetFCN",
    "parameters": {
      "num_classes": 1,
      "num_rgb": 2,
      "base": 32,
      "dropout_rates": [[0,0,0,0]]
    }
  },
  "optimizer": {
    "name": "ADAM",
    "parameters": {
      "lr": [0.0005],
      "weight_decay": [0.001, 0.0005, 0.0001]
    }
  },
  "scheduler": {"name": "ReduceLROnPlateau",
      "parameters": {"factor": 0.5,
       "patience": 15,
       "threshold": 0.1,
       "verbose": true
       }
  },
  "loss_function": {
    "name": "BCELoss"
  },
  "trainer_delegate": {
    "name": "StatOil"
  },
  "result_delegate": {
    "name": "StatOil"
  },
  "data_source_delegate": {
    "name": "StatOil",
    "parameters": {
      "batch_size": 40,
      "n_splits": 8,
      "splits_to_use": 8,
      "image_size": [75, 75],
      "data_handler_method": "TwoChannels",
      "testing_data_path": "/home/kevin/workspace/Kaggle/Iceberg-Classifier-Challenge/src/data/test.json",
      "training_data_path": "/home/kevin/workspace/Kaggle/Iceberg-Classifier-Challenge/src/data/train.json"
    }
  },
  "saver_delegate": {
    "name": "StatOil"
  },
  "evaluation_delegate": {
    "name": "StatOil"
  },
  "n_epochs": 80
}

